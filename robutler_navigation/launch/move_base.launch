<?xml version="1.0"?>
<launch>

  <!--- Run Move Base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" >

    
    <rosparam file="$(find robutler_navigation)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find robutler_navigation)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find robutler_navigation)/params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find robutler_navigation)/params/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find robutler_navigation)/params/base_local_planner_func.yaml" command="load"/>
    
     
  </node>

</launch>

